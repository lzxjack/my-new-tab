{
  "manifest_version": 3,
  "name": "My New Tab",
  "version": "1.0.0",
  "description": "自定义新标签页",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_icon": {
      "16": "logo.png",
      "24": "logo.png",
      "32": "logo.png"
    },
    "default_title": "My New Tab"
  }
}
